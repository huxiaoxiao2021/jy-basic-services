<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jsf="http://jsf.jd.com/schema/jsf"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd">

    <jsf:registry id="jsfRegistry" index="i.jsf.jd.com" protocol="jsfRegistry"/>

    <jsf:server id="jsf" protocol="jsf" threads="400"/>

    <!-- 登陆-用户回调接口
    <jsf:provider alias="${jsf.ql.springboot.alias}" id="userCallbackJsfServiceSDK"
                  interface="com.jd.intl.taskhub.callback.jsf.user.UserCallbackJsfService"
                  ref="userCallbackJsfServiceImpl"
                  server="jsf"/> -->
    <!--  集箱包裹数量限制JSF服务   -->
    <jsf:provider id="boxlimitConfigApi"
                  interface="com.jdl.basic.api.service.BoxlimitConfigApi"
                  alias="${jsf.alias.jy_basic}"
                  ref="boxlimitConfigApiImpl"
                  server="jsf">
    </jsf:provider>


</beans>

