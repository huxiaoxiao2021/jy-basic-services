    <jsf:provider id="configTransferDpBusinessApiJsf"
                  interface="com.jdl.basic.api.service.transferDp.ConfigTransferDpBusinessApi"
                  alias="${jsf.jy_basic.alias}"
                  ref="configTransferDpBusinessApiImpl"
